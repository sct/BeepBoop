{
  "name": "beepboop",
  "version": "0.1.1",
  "description": "Soundboard bot for Discord",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js"
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-parameters",
      "transform-es2015-duplicate-keys",
      "transform-es2015-modules-commonjs",
      "transform-exponentiation-operator",
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "arrow-parens": "off",
      "generator-star-spacing": "off",
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sct/BeepBoop.git"
  },
  "author": "Ryan \"sct\" Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sct/BeepBoop/issues"
  },
  "homepage": "https://github.com/sct/BeepBoop#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-types": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "bufferutil": "^3.0.0",
    "cors": "^2.8.3",
    "discord.js": "^11.1.0",
    "dotenv": "^4.0.0",
    "erlpack": "^0.1.2",
    "express": "^4.15.3",
    "express-jwt": "^6.1.2",
    "jsonwebtoken": "^7.4.1",
    "libsodium-wrappers": "^0.5.1",
    "mime": "^1.3.6",
    "mongoose": "^4.10.0",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-discord": "^0.1.2",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "uws": "^0.14.5",
    "winston": "^2.3.1"
  }
}
